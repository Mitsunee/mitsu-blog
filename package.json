{
  "name": "mitsublog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "simple-git-hooks",
    "lint": "eslint .",
    "format": "prettier -w .",
    "dev": "next dev",
    "dev:types": "tsc --noEmit --watch",
    "typecheck": "tsc --noEmit",
    "prebuild": "node prebuild.mjs",
    "build": "next build",
    "start": "next start"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "nano-staged": {
    "**/*.{js,jsx,mjs,cjs,ts,tsx}": [
      "eslint",
      "prettier -w"
    ],
    "**/*.{css,json,md,yml}": [
      "prettier -w"
    ]
  },
  "browserslist": [
    "last 2 versions",
    ">1%",
    "not dead"
  ],
  "dependencies": {
    "@foxkit/node-util": "^0.4.0",
    "@foxkit/util": "^0.5.2",
    "@types/node": "^18.7.6",
    "@types/react": "^18.0.17",
    "classcat": "^5.0.3",
    "globby": "^13.1.2",
    "iconoir": "^5.1.4",
    "modern-diacritics": "^2.3.0",
    "next": "12.2.5",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype": "^12.0.1",
    "rehype-external-links": "^2.0.0",
    "rehype-preset-minify": "^6.0.0",
    "rehype-react": "^7.1.1",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-breaks": "^3.0.2",
    "remark-extract-frontmatter": "^3.2.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gemoji": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-prism": "^1.3.6",
    "remark-rehype": "^10.1.0",
    "remark-twemoji": "^0.1.1",
    "spacetime": "^7.1.4",
    "twemoji": "^14.0.2",
    "typescript": "^4.7.4",
    "unified": "^10.1.2",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "^8.22.0",
    "eslint-config-foxkit": "^1.1.1",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "nano-staged": "^0.8.0",
    "prettier": "^2.7.1",
    "simple-git-hooks": "^2.8.0"
  }
}
